#include <iostream>

double calculateAverage(int grades[4]) {
    return (grades[0] + grades[1] + grades[2] + grades[3]) / 4.0;
}

int main() {
    int grades[4] = {80, 90, 75, 95};
    double average = calculateAverage(grades);
    std::cout << "Average grade: " << average << std::endl;

    return 0;
}